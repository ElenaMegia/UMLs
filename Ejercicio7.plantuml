@startuml
class "Proyecto " as Proyecto{
    Nombre: "1 Texto"
    Fecha de inicio: 1 Tiempo
    Fecha de fin: 0..1 Tiempo
}
class "Miembros del Equipo" as Miembros{
   Nombre: 1 Texto
   Apellidos: 1 Texto
   Rol: 0..* Texto

}
class "Lugar de Actuacion" as Lugar{
    Nombre: 0..* Texto
    Coordenada X: 1 Número
    Coordenada Y: 1 Número

}
@enduml